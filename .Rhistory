library(tidyverse)
library(zoo)
library(data.table)
library(obstinit)
obi = data.table::fread("P:/OBI_abstracted_data/Current_Data/data/output/sourcetables_OBI_export_recodes.csv")
obi_cohort = obi %>% mutate(infant_dob_dt = lubridate::dmy_hms(infant_dob_dt)) %>% filter(
birth_year > 2019,
flg_complete == 1,
infant_dob_dt <= lubridate::ymd_hms("2022-11-30 23:59:59")
)
dt_for_ctrl_chart = obi_cohort %>% structure_data(
infant_dob_dt,
date_gran = year_mon,
overall_dystocia_compliance_num,
overall_dystocia_den_all,
long = F
)
View(dt_for_ctrl_chart)
ctrl_chart_plot = plot_ctrl_chart(dt_for_ctrl_chart)
ctrl_chart_plot
